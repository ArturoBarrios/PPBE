generator client {
    provider = "prisma-client-js"
  }
  
  datasource db {
    provider = "postgresql" // or "sqlite" or "mysql" depending on what you're using
    url      = env("DATABASE_URL")
  }
  
  model User {
    id    String  @id @default(uuid())
    email String  @unique
    name  String?
  }
  